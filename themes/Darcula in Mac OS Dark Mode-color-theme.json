      "settings": {
        "foreground": "#FFC66D"
      }
    },
    {
      "scope": "meta.preprocessor",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": "meta.preprocessor.string",
      "settings": {
        "foreground": "#ce9178"
      }
    },
    {
      "scope": "meta.preprocessor.numeric",
      "settings": {
        "foreground": "#b5cea8"
      }
    },
    {
      "scope": "meta.structure.dictionary.key.python",
      "settings": {
        "foreground": "#9876AA"
      }
    },
    {
      "scope": "meta.diff.header",
      "settings": {
        "foreground": "#CC7832"
      }
    },
    {
      "scope": "storage",
      "settings": {
        "foreground": "red"
      }
    },
    {
      "scope": "storage.type",
      "settings": {
