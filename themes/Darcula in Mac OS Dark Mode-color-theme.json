{
  "$schema": "vscode://schemas/color-theme",
  "name": "Darcula in Mac OS Dark Mode",
  "type": "dark",
  "colors": {
    "focusBorder": "#ffc600",
    // activityBar
    "activityBar.background": "#454545",
    "activityBar.border": "#2d2d2d",
    "activityBar.dropBackground": "#454545",
    "activityBar.foreground": "#fff",
