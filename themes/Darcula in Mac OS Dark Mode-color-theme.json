        "variable.other.jsdoc"
      ],
      "settings": {
        "foreground": "#808080"
      }
    },
    {
      "scope": "storage.modifier",
      "settings": {
        "foreground": "#CC7832",
        "fontStyle": "bold"
      }
    },
    {
      "scope": "string",
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "scope": "string.tag",
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "scope": "string.value",
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "scope": "string.regexp",
      "settings": {
        "foreground": "#6A8759"
      }
    },
    {
      "name": "JavaScript string interpolation ${}",
      "scope": [
        "punctuation.definition.template-expression.begin.js",
