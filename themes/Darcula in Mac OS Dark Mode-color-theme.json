    "peekViewEditorGutter.background": "#212122",
    "peekViewResult.background": "#2d2d2d",
    "peekViewResult.fileForeground": "#aaa",
    "peekViewResult.lineForeground": "#fff",
    "peekViewResult.matchHighlightBackground": "#454545",
    "peekViewResult.selectionBackground": "#454545",
    "peekViewResult.selectionForeground": "#fff",
    "peekViewTitle.background": "#2d2d2d",
    "peekViewTitleDescription.foreground": "#aaa",
    "peekViewTitleLabel.foreground": "#ffc600",
    // picker
    "pickerGroup.border": "#454545",
    "pickerGroup.foreground": "#aaa",
    // progressBar
    "progressBar.background": "#ffc600",
    // scrollbar
    "scrollbar.shadow": "#00000000",
    "scrollbarSlider.activeBackground": "#454545cc",
    "scrollbarSlider.background": "#2d2d2d80",
    "scrollbarSlider.hoverBackground": "#454545cc",
    // selection
    "selection.background": "#454545",
    // sidebar
    "sideBar.background": "#2d2d2d",
    "sideBar.border": "#454545",
    "sideBar.foreground": "#aaa",
    "sideBarSectionHeader.background": "#2d2d2d",
    "sideBarSectionHeader.foreground": "#fff",
    "sideBarTitle.foreground": "#aaaaaa",
    // statusBar
    "statusBar.background": "#2d2d2d",
    "statusBar.border": "#454545",
    "statusBar.debuggingBackground": "#2d2d2d",
    "statusBar.debuggingForeground": "#454545",
    "statusBar.foreground": "#aaa",
    "statusBar.noFolderBackground": "#2d2d2d",
    "statusBar.noFolderForeground": "#aaa",
    "statusBarItem.activeBackground": "#2d2d2d",
    "statusBarItem.hoverBackground": "#454545",
    "statusBarItem.prominentBackground": "#2d2d2d",
    "statusBarItem.prominentHoverBackground": "#454545",
    // tab
    "tab.activeBackground": "#454545",
    "tab.activeForeground": "#fff",
    "tab.border": "#2d2d2d",
    "tab.activeBorder": "#454545",
    "tab.hoverBackground": "#454545",
    "tab.inactiveBackground": "#2d2d2d",
    "tab.inactiveForeground": "#aaa",
    "tab.unfocusedActiveForeground": "#aaa",
    "tab.unfocusedInactiveForeground": "#aaa",
