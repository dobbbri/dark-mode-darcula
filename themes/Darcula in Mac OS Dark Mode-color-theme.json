    "editorWarning.border": "#ffffff00",
    "editorWarning.foreground": "#ffc600",
    "editorWhitespace.foreground": "#ffffff1a",
    "editorWidget.background": "#2d2d2d",
    "editorWidget.border": "#454545",
    "errorForeground": "#A22929",
