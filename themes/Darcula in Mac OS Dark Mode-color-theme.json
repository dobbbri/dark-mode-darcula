    "gitDecoration.modifiedResourceForeground": "#ffc600",
    "gitDecoration.deletedResourceForeground": "#ff628c",
    "gitDecoration.untrackedResourceForeground": "#3ad900",
    "gitDecoration.ignoredResourceForeground": "#ff628c",
    "gitDecoration.conflictingResourceForeground": "#FF7200",
    // textBlockQuote
    "textBlockQuote.background": "#212122",
    "textBlockQuote.border": "#454545",
    "textCodeBlock.background": "#212122",
    "textLink.activeForeground": "#0088ff",
    "textLink.foreground": "#0088ff",
    "textPreformat.foreground": "#ffc600",
    "textSeparator.foreground": "#454545",
    "titleBar.border": "#454545",
    "titleBar.activeBackground": "#2d2d2d",
    "titleBar.activeForeground": "#ffffff",
    "titleBar.inactiveBackground": "#212122",
    "titleBar.inactiveForeground": "#ffffff33",
    "walkThrough.embeddedEditorBackground": "#454545",
    "welcomePage.buttonBackground": "#212122",
    "welcomePage.buttonHoverBackground": "#454545",
    "widget.shadow": "#00000026"
