    "editor.hoverHighlightBackground": "#ffc60033",
    // when you have something selected, but have lost focus on the editor
    "editor.inactiveSelectionBackground": "#212122",
    // current line styles
    "editor.lineHighlightBackground": "#454545",
    "editor.lineHighlightBorder": "#454545",
    "editor.rangeHighlightBackground": "#454545",
